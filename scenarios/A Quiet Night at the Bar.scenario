            <prompt id="0" parentid="" text="Hey @!PLAYER!&lt;br&gt;&lt;br&gt; I'm so glad to see you.&#10;It has been a while! How are you doing?" character="Eryn" background="pub.jpg" isend="false">
                  <reply text="Eryn! What is up buddy? I'm doing well. What about you?" target="22eca69d-fd28-4bda-a3d8-7f38b730db47"/>
                  <reply text="Hey, Eryn. I'm all right. What about you?" target="b6b988b4-88e4-4fd8-8e23-c23b0f880558"/>
                  <reply text="Meh. What about you?" target="b0674f59-bbaa-4d99-9329-96167faeacb4"/>
            </prompt>
