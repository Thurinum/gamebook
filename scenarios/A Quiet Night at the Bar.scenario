            <prompt id="ad6837a3-6a78-4706-bb07-a0d5687395c5" parentid="0" text="(NEW PROMPT)" character="" background="" isend="false"/>
            <prompt id="9d3695a6-99eb-4c21-9a3f-0a763e225196" parentid="886a78b2-c1f6-4dce-806a-83ec8c7bd094" text="It's all right. Haha. How about we eat something? I feel in the mood for eating." character="Eryn" background="pub.jpg" isend="false">
